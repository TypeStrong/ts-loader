{
    "compilerOptions": {
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": false,
        "lib": [
            "dom", "es2015" // dom for console.log only
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "../dist"
    }
}