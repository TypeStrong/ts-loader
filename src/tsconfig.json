{
    "compilerOptions": {
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "lib": [
            "es5", "es2015.core"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "../dist"
    }
}