{
    "compilerOptions": {
        "allowJs": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "target": "es2015",
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "skipLibCheck": true
    }
}